Input = {"string": "AAAAAAAAAAAAABBCCCCDD"}
output is "9A4A2B4C2D"


def runLengthEncoding(string):
	count =1
	i=1 
	res=''
	while i<len(string):
		if count==9:
			res=res+str(count)+string[i-1]
			count =1 
		elif string[i]!= string[i-1]:
			res=res+ str(count) + string[i-1]
			count=1
		else:
			count+=1
		i+=1
	return res+str(count)+string[i-1]
